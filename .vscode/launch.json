// The format of this file is specified in https://probe.rs/docs/tools/debugger/#start-a-debug-session-with-minimum-configuration
{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "Debug RP2040 Pico",
            "chip": "RP2040",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "rust: cargo build",
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "${workspaceFolder}/target/thumbv6m-none-eabi/debug/blinky",
                    "rttEnabled": true
                }
            ]
        }
    ]
}